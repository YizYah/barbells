{
  "name": "barbells",
  "version": "1.0.0",
  "description": "simple management of helpers and partials for handlebars",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"src/**/*.ts\"",
    "lintfix": "eslint \"src/**/*.ts\" --fix",
    "test": "ava",
    "posttest": "eslint \"src/**/*.ts\"",
    "prepack": "rm -rf lib && tsc -b"
  },
  "keywords": [
    "handlebars",
    "helpers",
    "partials",
    "ts-packrat"
  ],
  "author": "YizYah",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^14.14.19",
    "fs-extra": "^9.1.0",
    "handlebars": "^4.7.7",
    "handlebars-helpers": "^0.10.0",
    "magicalstrings": "^0.0.14",
    "path": "^0.12.7",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "ava": "^3.15.0",
    "eslint": "^7.17.0",
    "nyc": "^14.1.1",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1"
  },
  "bugs": "https://github.com/YizYah/barbells/issues",
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "lib/**/*"
  ],
  "homepage": "https://github.com/YizYah/barbells",
  "types": "lib/index.d.ts",
  "repository": "YizYah/barbells",
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
